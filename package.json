    "protractor": "^3.3.0",
    "rimraf": "^2.5.2",
    "tslint": "^3.7.4",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  },
  "repository": {},
  "engines": {
    "node": "4.4.4",
    "npm": "3.9.0"
  }
